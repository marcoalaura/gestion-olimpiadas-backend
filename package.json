{
  "name": "min-educacion-olimpiadas-backend",
  "version": "0.1.0",
  "description": "",
  "author": "agetic@agetic.gob.bo",
  "private": true,
  "license": "LPG-Bolivia",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copy:statics",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "setup": "rm database/migrations/*; npm run schema:drop;npm run migrations:generate init;npm run migrations:run;npm run seeds:run",
    "start": "nest start && npm run copy:statics",
    "start:dev": "NODE_ENV=development npm run copy:statics && nest start --watch",
    "start:database": "./scripts/database.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "TZ=UTC jest --verbose --coverage=false",
    "test:single": "TZ=UTC NODE_ENV=test jest -i",
    "test:watch": "TZ=UTC NODE_ENV=test jest --watch",
    "test:cov": "TZ=UTC NODE_ENV=test jest --coverage",
    "test:debug": "TZ=UTC NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "TZ=UTC NODE_ENV=test jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:generate": "npm run typeorm -- migration:generate -c connection -n",
    "migrations:run": "npm run typeorm -- migration:run -c connection",
    "migrations:revert": "npm run typeorm -- migration:revert -c connection",
    "seeds:create": "npm run typeorm -- migration:create -c seed -n",
    "seeds:run": "npm run typeorm -- migration:run -c seed",
    "seeds:runfake": "npm run typeorm -- migration:run -c seed-fake",
    "seeds:runfakeTest": "npm run typeorm -- migration:run -c seed-fake-test",
    "seeds:runCargaFake": "npm run typeorm -- migration:run -c seed-carga-fake",
    "seeds:cargaEmpaquetado": "npm run typeorm -- migration:run -c seed-carga-empaquetado",
    "seeds:cargaExamen": "npm run typeorm -- migration:run -c seed-carga-examen",
    "seeds:cargaResultados": "npm run typeorm -- migration:run -c seed-carga-resultados",
    "seeds:performance": "npm run setup ; npm run typeorm -- migration:run -c seed-carga-fake ;npm run typeorm -- migration:run -c seed-carga-examen; npm run typeorm -- migration:run -c seed-carga-empaquetado; npm run typeorm -- migration:run -c seed-carga-resultados",
    "schema:drop": "npm run typeorm -- schema:drop -c connection",
    "rimraf": "./node_modules/rimraf/bin.js",
    "copy:statics": "cpx 'src/core/config/authorization/*.{conf, csv}' 'dist/src/core/config/authorization'",
    "compodoc": "compodoc -p tsconfig.json -o --language es-ES"
  },
  "dependencies": {
    "@alvaromq/carbone": "^3.3.0",
    "@golevelup/ts-jest": "^0.3.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.7.16",
    "@nestjs/typeorm": "^7.1.5",
    "@types/bcrypt": "^3.0.1",
    "@types/multer": "^1.4.5",
    "@types/rijndael-js": "^1.0.0",
    "bcrypt": "^5.0.1",
    "chmodr": "^1.2.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "cls-rtracer": "^2.6.0",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "javascript-state-machine": "^3.1.0",
    "jimp": "^0.16.1",
    "nanoid": "^3.1.22",
    "nest-authz": "^2.1.0",
    "nestjs-pino": "^1.4.0",
    "normalize-url": "^4.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pino-http-send": "^0.4.2",
    "pino-multi-stream": "^5.3.0",
    "pino-pretty": "^4.7.1",
    "reflect-metadata": "^0.1.13",
    "rijndael-js": "^2.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "tar": "^6.1.4",
    "typeorm": "^0.2.30",
    "typeorm-adapter": "^1.4.0",
    "typeorm-store": "^1.2.0",
    "wkhtmltopdf": "^0.3.4",
    "xlsx": "^0.17.0",
    "zxcvbn-typescript": "^5.0.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cpx": "^1.5.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "src/main.ts",
      "src/database/migrations/",
      "src/database/seeds/",
      ".module.ts",
      ".entity.ts",
      ".dto.ts",
      ".guard.ts"
    ],
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
